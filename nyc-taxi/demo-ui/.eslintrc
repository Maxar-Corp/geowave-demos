{
  "plugins": ["react"],
  "ecmaVersion": 6,
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },

  "globals": {
    L: true,
    angular: true
  },

  "extends": ["plugin:react/recommended"],
  "rules": {
    "comma-dangle": 1,
    "complexity": [1, 5],
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 1,
    "no-extra-parens": 0,
    "no-extra-semi": 1,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-lonely-if": 2,
    "no-shadow": 1,
    "no-shadow-restricted-names": 1,
    "no-sparse-arrays": 1,
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, {"functions": false, "classes": false}],
    "no-useless-concat": 1,
    "prefer-const": 1,
    "quotes": [1, "single", "avoid-escape"],
    "valid-typeof": 2
  }
}
